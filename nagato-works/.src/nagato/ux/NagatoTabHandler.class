Public Sub NewTab(argFunctionType As Integer, Optional argStartUp As Boolean = False)
    
    Dim yukiNewTab As Object
    
    If Not argStartUp Then Inc HaruhiMain.MainTabStrip.Count
    
    With MikuruFunctionType
        Select Case argFunctionType
            Case .AppLauncher
                yukiNewTab = New NagatoAppLauncher(HaruhiMain.MainTabStrip) 
            Case .Dictionary
                yukiNewTab = New NagatoDictionary(HaruhiMain.MainTabStrip) 
            Case .FileSelector
                yukiNewTab = New NagatoFileSelector(HaruhiMain.MainTabStrip) 
            Case .HighlightEditor
                yukiNewTab = New NagatoHighlightEditor(HaruhiMain.MainTabStrip) 
            Case .ImageViewer
                yukiNewTab = New NagatoImageViewer(HaruhiMain.MainTabStrip) 
            Case .Map
                yukiNewTab = New NagatoMapView2(HaruhiMain.MainTabStrip) 
            Case .MediaPlayer
                yukiNewTab = New NagatoMediaPlayer(HaruhiMain.MainTabStrip)
            Case .PdfViewer
                yukiNewTab = New NagatoPdfViewer(HaruhiMain.MainTabStrip) 
            Case .TextEditor
                yukiNewTab = New NagatoTextEditor(HaruhiMain.MainTabStrip) 
            Case .WebBrowser
                yukiNewTab = New NagatoWebBrowser(HaruhiMain.MainTabStrip) 
            Case .WebBookmarkEditor
                yukiNewTab = New NagatoWebBookmarkEditor(HaruhiMain.MainTabStrip) 
            Case .WebRecentPagesEditor
                yukiNewTab = New NagatoWebRecentPagesEditor(HaruhiMain.MainTabStrip) 
            Case .PureTextEditor
                yukiNewTab = New NagatoPureTextEditor(HaruhiMain.MainTabStrip)
            Case .TrashBin
                yukiNewTab = New NagatoTrashBin(HaruhiMain.MainTabStrip)
        End Select
    End With
    
End

