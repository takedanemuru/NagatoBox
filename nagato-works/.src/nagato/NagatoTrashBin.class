Inherits NagatoTabContent

Private $toolBar As NagatoTrashBinToolBar
Private $iconView As NagatoTrashBinIconView

Public Sub _new(argTabStrip As TabStrip)
    
    Me._Initialize(argTabStrip)
    $toolBar = New NagatoTrashBinToolBar(Me._$hBox) As "Toolbar"
    $iconView = New NagatoTrashBinIconView(Me._$vBox) As "IconView"
    Me._RefreshParentTab(("Trash Bin"), Stock["small/trash"])
    NagatoMessaging.setMessage(("...Trash Bin mode."))
    
End
