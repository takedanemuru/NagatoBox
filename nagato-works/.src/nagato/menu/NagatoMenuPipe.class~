' Gambas class file

Create Private

Event MenuEvent(argEvent As Integer)
Event MenuEventWithArgument(argEvent As Integer, argArgument As Variant)
Event MenuEventWithArguments(argEvent As Integer, argArguments As Variant[])

Public Sub Menu_MenuEvent(argEvent As Integer)
    
    Raise MenuEvent(argEvent)
    
End

Public Sub Menu_MenuEventWithArgument(argEvent As Integer, argArgument As Variant)
    
    Raise MenuEventWithArgument(argEvent, argArgument)
    
End

Public Sub Menu_MenuEventWithArguments(argEvent As Integer, argArguments As Variant[])
    
    Raise MenuEventWithArguments(argEvent, argArguments)
    
End
