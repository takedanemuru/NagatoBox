Private Sub addAppLauncherMenu()
    
    Dim yukiMenu As New Menu(Me._$rootMenu) As "Menu"
    
    With yukiMenu
        .Picture = Stock["medium/program"]
        .Text = ("AppLauncher")
        .Tag = MikuruFunctionType.AppLauncher
    End With
    
End

