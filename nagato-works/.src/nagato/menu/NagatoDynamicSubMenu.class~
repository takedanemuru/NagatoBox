' Gambas class file

Inherits NagatoSubMenu

Private $observer As Observer

Public Sub _RefreshMenu() ' This method is protected/virtual, MUST be overridden.
    
    
    
End

Public Sub _InitializeMenu() ' override
    
    Dim yukiDummyMenu As New Menu(Me._$rootMenu)
    
    yukiDummyMenu.Text = ""
    
    $observer = New Observer(Me._$rootMenu) As "Asakura"
    
End

Public Sub Asakura_Show()
    
    Me._$rootMenu.Children.Clear()
    Me._RefreshMenu()
    
End
