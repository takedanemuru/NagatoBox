' Gambas class file

Inherits NagatoTabContent

Private $toolBar As NagatoWebBookmarkEditorToolBar
Private $treeView As NagatoWebBookmarkEditorTreeView

Public Sub _new(argTabStrip As TabStrip)
    
    Me._Initialize(argTabStrip)
    $toolBar = New NagatoWebBookmarkEditorToolBar(Me._$hBox) As "ToolBar"
    $treeView = New NagatoWebBookmarkEditorTreeView(Me._$vBox) As "TreeView"
    Me._RefreshParentTab(("EditBookmark"), Stock["small/bookmark"])
    
End
