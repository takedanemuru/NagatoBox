' Gambas class file

Inherits NagatoToolBar

Event Back
Event Forward

Private Sub addZoomButtons()
    
    Dim yukiButtons As NagatoZoomButtons
    
    yukiButtons = New NagatoZoomButtons(Me._$parent) As "Menu"
    
End

Private Sub addGearButton()
    
    Dim yukiGearButton As NagatoGearButton
    Dim yukiMenu As NagatoPdfViewerMenu
    
    yukiGearButton = New NagatoGearButton(Me._$parent) 
    yukiMenu = New NagatoPdfViewerMenu(yukiGearButton.RootMenu) As "GearMenu"
    
End

Public Sub _Initialize() ' Override.
    
    Dim yukiGoBackButton As NagatoGoBackButtons
    Dim yukiSeparator As NagatoSeparator
    
    yukiGoBackButton = New NagatoGoBackButtons(Me._$parent) As "GoBackButton"
    addZoomButtons()
    yukiSeparator = New NagatoSeparator(Me._$parent)
    addGearButton()
    
End

Public Sub GoBackButton_Back()
    
    Raise Back
    
End

Public Sub GoBackButton_Forward()
    
    Raise Forward
    
End
