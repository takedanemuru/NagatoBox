' Gambas class file

Inherits NagatoToolButtons

Private $IsExpand As Boolean

Private Sub addSeparator()
    
    Dim yukiSeparator As New ToolButton(Me._$parent)
    
    With yukiSeparator
        .Height = Me.$size
        .Enabled = False
        .Expand = $IsExpand
    End With
    
End

Public Sub _new(argContainer As Container, Optional argIsExpand As Boolean = True)
    
    Me.initialize(argContainer)
    $IsExpand = argIsExpand
    addSeparator()
    
End
