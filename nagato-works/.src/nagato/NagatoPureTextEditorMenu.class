' Gambas class file

Inherits NagatoSuperMenu

Private Sub addFileMenu()
    
    Dim yukiMenu As New Menu(Me._$rootMenu) 
    Dim yukiSubMenu As NagatoSubMenuFile
    
    With yukiMenu
        .Text = ("File")
    End With
    
    yukiSubMenu = New NagatoSubMenuFile(yukiMenu) As "Menu"
    
End

Private Sub addPreferenceMenu()
    
    Dim yukiMenu As New Menu(Me._$rootMenu) 
    Dim yukiSubMenu As NagatoDynamicSubMenuPureTextEditorPreference
    
    With yukiMenu
        .Text = ("Preference")
    End With
    
    yukiSubMenu = New NagatoDynamicSubMenuPureTextEditorPreference(yukiMenu) As "Menu"
    
End

Public Sub _AddOtherMenu() ' override
    
    addFileMenu()
    addPreferenceMenu()
    
End
