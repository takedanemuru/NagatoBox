Inherits NagatoSuperMenuButton

Public Sub initializeWidget()
    
    Me.$button = New MenuButton(Me._$parent) As "ImageEffectButton" 
    
    With Me.$button
        .Arrow = False
        .Border = False
        .Width = Me.$size
        .Picture = Stock["medium/color"]
        .Tooltip = ("Image Effect")
    End With
    
End
