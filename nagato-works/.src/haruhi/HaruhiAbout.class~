' Gambas class file

Public Sub Form_Open()
    
    Me.Center()
    Me.Height = 48 * MikuruUx.Grid
    Me.Width = 32 * MikuruUx.Grid
    AboutVBox.Padding = 2 * MikuruUx.Grid
    IconPictureBox.Picture = Picture.Load("pictures/icon.png")
    AboutTextLabel.Text = Subst$(File.Load("text/AboutText"), Application.Version)
    
End
