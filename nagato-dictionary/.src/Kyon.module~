' Gambas module file

Public Sub Main()
    
    NagatoSettings.EnsureDictionaryData()
    NagatoArgs.Parsing
    
    Print "\t" & "YUKI.N"
    Print "\t\t" & "YUKI.N"
    Print "\t\t\t" & "YUKI.N"
    Print "\t\t\t\t" & "YUKI.N"
    
    Debug "YUKI.N > Activate generaldict table on " & NagatoDBGeneralDict.HostName
    
    If NagatoArgs.Queries.Count > 0 Then 
        NagatoCommandlineInterface.ShowResults(NagatoArgs.Queries)
    Else
        HaruhiMain.Show()
    End If
    
End
