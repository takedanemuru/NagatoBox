Inherits NagatoObject

Public _$tableView As TableView
Public _$observer As Observer

Public Sub _PaintRow(argRow As Integer)
    
    Dim yukiColumnIndex As Integer
    
    For yukiColumnIndex = 0 To Me._$tableView.Columns.Max
        Me._$tableView[argRow, yukiColumnIndex].Background = MikuruColor.GridBackground
    Next
    
End

Public Sub _OptionalOnInitialize()
    
    
    
End

Public Sub _new(argTableView As TableView)
    
    Me._$tableView = argTableView
    Me._$observer = New Observer(argTableView) As "Asakura"
    Me._OptionalOnInitialize()
    
End
