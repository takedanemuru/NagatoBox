' Gambas module file

Public Sub _call(argTabTitle As String, argUrl As String)
    
    With HaruhiAddWebBookmark
        .SetTitle(argTabTitle)
        .SetList(NagatoDBBookmark.CategoryNames)
        If .ShowDialog() <> 1 Then Return
        NagatoDBBookmark.AddNewBookmark(argUrl, argTabTitle, .UserTitle, .Category)
    End With
    
End
