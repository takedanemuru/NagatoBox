' Gambas module file

Public Sub Initialize()
    
    With WebSettings
        .[.PluginsEnabled] = True
        .[.JavascriptEnabled] = True
        .[.JavaEnabled] = True
        .[.JavascriptCanOpenWindows] = True
        .[.DeveloperExtrasEnabled] = True
        .[.JavascriptCanAccessClipboard] = True
        .[.LocalStorageDatabaseEnabled] = True
    End With
    
End
