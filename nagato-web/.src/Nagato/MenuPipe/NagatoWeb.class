' Gambas class file

Inherits NagatoMenuPipe

Property Read UniqueIndex As Integer
Property Read Title As String
Property Read Url As String
Property Read HistoryMax As Integer
Property Read HistoryIndex As Integer

Private $uniqueIndex As Integer
Private $webView2 As NagatoWebView

Public Sub Forward()
    
    $webView2.Forward()
    
End

Public Sub Back()
    
    $webView2.Back()
    
End

Public Sub SetUrl(argUrl As String)
    
    $webView2.SetUrl(argUrl)
    
End

Public Sub Delete()
    
    $webView2.Delete()
    
End

Public Sub Toggle(argActiveTabIndex As Integer)
    
    $webView2.Toggle(argActiveTabIndex = Me.UniqueIndex)
    
End

Public Sub InPageSearch(argQuery As String)
    
    $webView2.InPageSearch(argQuery)
    
End

Public Sub _new(argContainer As Container, argStartupUrl As String, argUniqueIndex As Integer)
    
    $uniqueIndex = argUniqueIndex
    $webView2 = New NagatoWebView(argContainer, argStartupUrl, argUniqueIndex) As "Menu"
    
End

Private Function UniqueIndex_Read() As Integer

    Return $uniqueIndex

End

Private Function Title_Read() As String

    Return $webView2.WebView.Title

End

Private Function Url_Read() As String

    Return $webView2.WebView.Url

End

Private Function HistoryMax_Read() As Integer

    Return $webView2.WebView.History.Max

End

Private Function HistoryIndex_Read() As Integer

    Return $webView2.WebView.History.Index

End
