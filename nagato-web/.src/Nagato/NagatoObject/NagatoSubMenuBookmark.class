' Gambas class file

Inherits NagatoObject

Private $rootMenu As Menu

Public Sub Refresh()
    
    Dim yukiCategory As String
    Dim yukiMenu As NagatoSubMenuBookmarkCategory
    
    For Each yukiCategory In NagatoDBBookmark.CategoryNames.Sort(gb.Natural + gb.IgnoreCase)
        If String.InStr(yukiCategory, "/", 0) > 0 Then Continue
        yukiMenu = New NagatoSubMenuBookmarkCategory($rootMenu, yukiCategory) As "TFEI"
    Next
    
End

Public Sub _new(argRootMenu As Menu)
    
    $rootMenu = argRootMenu
    
    Me.Refresh()
    
End
