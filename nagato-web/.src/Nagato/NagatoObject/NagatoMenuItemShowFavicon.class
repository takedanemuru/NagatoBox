' Gambas class file

Inherits NagatoMenuItem

Public Sub _OnInitializeMenuItem()
    
    With Me._$menuItem
        .Text = ("Show Favicon")
        .Picture = MikuruIcon.GetCheckButton(NagatoSettings.ShowFavicon)
    End With
    
End

Public Sub _OnClickMenuItem()
    
    NagatoSettings.ShowFavicon = Not NagatoSettings.ShowFavicon
    
End