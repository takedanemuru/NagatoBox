Inherits NagatoPullDownMenu

Public Sub _OnShowRootMenu()
    
    Dim yukiMenu As NagatoMenuItemChangePersona
    
    yukiMenu = New NagatoMenuItemChangePersona(Me._$rootMenu) As "TFEI"
    
    Object.New("NagatoMenuItemShowFavicon", [Me._$rootMenu])
    Object.New("NagatoMenuItemPluginsEnabled", [Me._$rootMenu])
    Object.New("NagatoMenuItemJavaScriptEnabled", [Me._$rootMenu])
        MikuruMenuSeparator.Set(Me._$rootMenu)
    Object.New("NagatoMenuItemAbout", [Me._$rootMenu])
    Object.New("NagatoMenuItemQuit", [Me._$rootMenu])
    
End
