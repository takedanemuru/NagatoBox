Inherits NagatoMenuItem

Public Sub _OnInitializeMenuItem()
    
    With Me._$menuItem
        .Text = ("Plugins Enabled")
        .Picture = MikuruIcon.GetCheckButton(NagatoSettings.PluginsEnabled)
    End With
    
End

Public Sub _OnClickMenuItem()
    
    NagatoSettings.PluginsEnabled = Not NagatoSettings.PluginsEnabled
    WebSettings[WebSettings.PluginsEnabled] = NagatoSettings.PluginsEnabled
    
End
