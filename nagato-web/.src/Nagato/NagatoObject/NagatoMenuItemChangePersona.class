' Gambas class file

Inherits NagatoMenuItem

Public Sub _OnInitializeMenuItem()
    
    With Me._$menuItem
        .Text = ("Change Persona")
        .Picture = MikuruIcon.Get("image")
    End With
    
End

Public Sub _OnClickMenuItem()
    
    If HaruhiImageSelect.ShowDialog() <> MikuruDialogResponse.Apply Then Return
    
    Raise Signal(MikuruSignal.WebRequestRefresh, Null)
    
End
