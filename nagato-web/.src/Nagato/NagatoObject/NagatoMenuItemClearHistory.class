' Gambas class file

Inherits NagatoMenuItem

Public Sub _OnInitializeMenuItem()
    
    With Me._$menuItem
        .Text = ("Clear History")
        .Picture = MikuruIcon.Get("bin")
    End With
    
End

Public Sub _OnClickMenuItem()
    
    If Message.Delete(("YUKI.N > Are you sure to clear history ?"), ("Cancel"), ("Clear")) = 2 Then
        NagatoDBHistory.ClearAll()
    Endif
    
End
