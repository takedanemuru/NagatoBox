' Gambas class file

Inherits NagatoMenuItem

Public Sub _OnInitializeMenuItem()
    
    With Me._$menuItem
        .Text = ("Toggle In-page search")
        .Picture = MikuruIcon.Get("search")
        .Shortcut = "Ctrl+F"
    End With
    
End

Public Sub _OnClickMenuItem()
    
    HaruhiMain.GlobalShortcuts(MikuruSignal.InPageSearchToggle, Null)
    
End
