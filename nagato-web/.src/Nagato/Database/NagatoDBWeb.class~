' Gambas class file

Inherits NagatoDB

Property Read ChannelFavicon As NagatoDBWebAssistantFavion
Property Read ChannelHistory As NagatoDBWebAssistantHistory

Public _$uniqueName As String = "Web_bk"

Private $assistantFavicon As NagatoDBWebAssistantFavion
Private $assistantHistory As NagatoDBWebAssistantHistory

Public Sub _new()
    
    Me._SetConnection()

    $assistantFavicon = New NagatoDBWebAssistantFavion(Me._$connection)
    $assistantHistory = New NagatoDBWebAssistantHistory(Me._$connection)

End

Private Function ChannelFavicon_Read() As NagatoDBWebAssistantFavion

    Return $assistantFavicon

End

Private Function ChannelHistory_Read() As NagatoDBWebAssistantHistory

    Return $assistantHistory

End
