' Gambas module file

Public Sub _get(argIconName As String, Optional argSize As Integer = 16) As Picture
    
    Dim yukiImage As Image = Image.Load(Subst("pictures/Icons/&1.png", argIconName))
    
    Return yukiImage.Stretch(argSize, argSize).Picture
    
End

Public Sub GetActionIcon(argActionName As String) As Picture
    
    Return Me["arrow-right"]
    
End
