' Gambas class file

Inherits NagatoNodeIntermediate

Public Sub _OnInitialize(argXmlElement As XmlElement)
    
    Dim yukiAction As String = argXmlElement.GetAttribute("action")
    Dim yukiButton As String = argXmlElement.GetAttribute("button")
    
    Me._$text = yukiAction & " " & yukiButton
    Me._$columnView.Add(Me._$key, Me.Text, MikuruIcon["mouse"], Me._$parentKey)
    Me._SetChildren(argXmlElement.GetChildrenByTagName("action",, 2))
     
End