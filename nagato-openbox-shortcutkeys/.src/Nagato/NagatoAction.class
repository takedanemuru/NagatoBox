' Gambas class file

Inherits NagatoNodeIntermediate

Public Sub _OnInitialize(argXmlElement As XmlElement)
    
    Me._$text = argXmlElement.GetAttribute("name")
    If argXmlElement.Name = "startupnotify" Then Me._$text = "startup notify"
    If argXmlElement.Name = "position" Then Me._$text = "position"
    Me._$columnView.Add(Me._$key, Me._$text, MikuruIcon["arrow-right"], Me._$parentKey)
    Me._SetChildren(Me._$xmlElement.ChildElements)
    
End
