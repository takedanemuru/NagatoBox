Inherits NagatoObject

Public _$menuButton As MenuButton

Public Sub _initializeMenuButton(argContainer As Container)
    
    Me._$menuButton = New MenuButton(argContainer) As "MenuButton"
    
    With Me._$menuButton
        .Arrow = False
        .Border = False
        .W = argContainer.H
        .Expand = False
    End With
    
End

Public Sub _OptionalInitializeMenuButton(argContainer As Container)
    
    ' virtual
    
End

Public Sub _new(argContainer As Container)
    
    Me._initializeMenuButton(argContainer)
    Me._OptionalInitializeMenuButton(argContainer)
    
End
