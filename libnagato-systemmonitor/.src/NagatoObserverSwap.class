' Gambas class file

Inherits NagatoObserver

Public Function _GetUsage() As Float
    
    Dim yukiSnapShot As String[] = Split(File.Load("/proc/swaps"), "\n", "")
    Dim yukiSize As Integer = Split(yukiSnapShot[1], "\t", "", True)[1]
    Dim yukiUsed As Integer = Split(yukiSnapShot[1], "\t", "", True)[2]
    
    Return yukiUsed / yukiSize
    
Catch
    Return 0
    
End
