Private $charactor As Image
Private $backGround As Image

Private Sub initializeBackGroundImage()
    
    Dim yukiImage As New Image($charactor.H, $charactor.H, Color.Transparent)
    
    Paint.Begin(yukiImage)
        Paint.Brush = Paint.Color(13755366)
        Paint.Rectangle(100, 100, 320, 250, 20)
        Paint.Fill()
        Paint.DrawImage($charactor, 0, 0)
    Paint.End
    
    $backGround = yukiImage.Copy()
    
End

Public Sub _new()
    
    Dim yukiCharactor As Image = Image.Load("nagato_yuki__render__by_kyantsu-d6lgt5y.png")
    
    $charactor = yukiCharactor.Stretch(yukiCharactor.W * 0.5, yukiCharactor.H * 0.5)
    
    initializeBackGroundImage()
    
End

Public Sub Form_Show()
    
    Me.Picture = $backGround.Picture
    
End
