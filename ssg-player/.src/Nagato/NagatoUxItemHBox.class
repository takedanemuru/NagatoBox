Property Read Item As HBox

Private $hBox As HBox

Public Sub _Initialize(argContainer As Container) 
    
    $hBox = New HBox(argContainer)
    
    With $hBox
        .H = 24
        .Background = MikuruColor.Base
        .Expand = False
    End With
    
End

Private Function Item_Read() As HBox

    Return $hBox

End
