Inherits NagatoSettingsCore

Property Theme As Integer
Property SectionLevel As Integer

Private Function Theme_Read() As Integer

    Return Me._$settings["slide/theme", NagatoValueListCss.StyleLight]

End

Private Sub Theme_Write(Value As Integer)

    Me._$settings["slide/theme"] = Value
    Me._$settings.Save()

End

Private Function SectionLevel_Read() As Integer

    Return Me._$settings["slide/level", 2]

End

Private Sub SectionLevel_Write(Value As Integer)

    Me._$settings["slide/level"] = Value
    Me._$settings.Save()

End
