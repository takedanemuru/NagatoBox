Property Read Current As Integer
Property Read Previous As Integer

Private $current As Integer = -Inf
Private $previous As Integer = -Inf

Public Sub Push(argData As Integer)
    
    $previous = $current
    $current = argData
    
End

Private Function Current_Read() As Integer

    Return $current

End

Private Function Previous_Read() As Integer

    Return $previous

End
