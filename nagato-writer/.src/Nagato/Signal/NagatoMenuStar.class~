' Gambas class file

Inherits NagatoPullDownMenu

Property Read Name As String

Private Const UniqueName As String = "StarMenu"

Public Sub _AddChildMenus()
    
    Me._AddMenu("Preview", MikuruSignal.ViewPreview, "search")
    Object.New("NagatoMenuSettings", [Me._$parentMenu, ("Settings"), "equalizer"])
        Me._AddMenu("", 0)
    Me._AddMenu("About", MikuruSignal.AppAbout, "star")
    Me._AddMenu("Quit", MikuruSignal.AppQuit, "button-power")
    
End

Private Function Name_Read() As String

    Return UniqueName

End
