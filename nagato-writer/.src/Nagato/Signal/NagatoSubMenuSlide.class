' Gambas class file

Inherits NagatoSubMenu

Public Sub _AddChildMenus()
    
    Dim yukiThemeMenu As NagatoSubMenuSlideTheme
    Dim yukiLevelMenu As NagatoSubMenuSlideLevel
    
    Me._AddMenu(("Start Slide Show"), MikuruSignal.PublishDzslidePreview, "button-play")
            Me._AddMenu("", MikuruSignal.None)
    yukiThemeMenu = New NagatoSubMenuSlideTheme(Me._$parentMenu, ("Theme"), "color-palette")
    yukiLevelMenu = New NagatoSubMenuSlideLevel(Me._$parentMenu, ("Section Level"))

End
