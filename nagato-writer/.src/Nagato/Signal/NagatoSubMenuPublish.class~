' Gambas class file

Inherits NagatoSubMenu

Public Sub _AddChildMenus()
    
    Me._AddMenu(("PDF"), MikuruSignal.PublishPdf)
    Me._AddMenu(("HTML"), MikuruSignal.PublishHtml)
    Me._AddMenu(("Plain Text"), MikuruSignal.PublishPlainText)
        Me._AddMenu("", MikuruSignal.None)
    Me._AddMenu(("Slidy (Slide Show)"), MikuruSignal.PublishSlidy)
    
End