' Gambas class file

Inherits NagatoUxContainer

Private $recentMenu As NagatoRecentFilesMenu

Public Sub _initialize() ' override
    
    Dim yukiButton As New MenuButton(Me._$parentContainer)
    
    $recentMenu = New NagatoRecentFilesMenu(HaruhiMain, "RecentFilesMenu") As "TFEI"
    
    With yukiButton
        .Text = ""
        .Arrow = False
        .Border = False
        .AutoResize = True
        .Picture = MikuruIcon["alarm-clock"]
        .Text = ("Recent")
        .Menu = "RecentFilesMenu"
    End With
    
End
