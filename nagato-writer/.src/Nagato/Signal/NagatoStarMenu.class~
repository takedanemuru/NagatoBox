' Gambas class file

Inherits NagatoButtonMenu

Public Const UniqueName As String = "StarMenu"

Public Sub _RefreshChildMenus() ' override
    
    Dim yukiPublishMenu As NagatoSubMenuPublish
    
    Me._AddMenu("Preview", MikuruSignal.ViewPreview, "search")
    yukiPublishMenu = New NagatoSubMenuPublish(Me._$parentMenu, "Publish", "book-open-bookmark") As "TFEI"
    Object.New("NagatoSubMenuSettings", [Me._$parentMenu, ("Settings"), "equalizer"])
        Me._AddMenu("", 0)
    Me._AddMenu("About", MikuruSignal.AppAbout, "star")
    Me._AddMenu("Quit", MikuruSignal.AppQuit, "button-power")
    
End
