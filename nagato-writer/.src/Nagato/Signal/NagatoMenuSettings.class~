' Gambas class file

Inherits NagatoDynamicMenu

Public Sub _AddChildMenus()
    
    Dim yukiMenu As New Menu(Me._$parentMenu) As "MenuItemLivePreview"
    
    With yukiMenu
        .Text = ("Live Preview")
        .Picture = IIf(NagatoSettings.LivePreview, MikuruIcon["button-check"], Null)
    End With
    
End

Public Sub MenuItemLivePreview_Click()
    
    NagatoSettings.LivePreview = Not NagatoSettings.LivePreview
    
End
