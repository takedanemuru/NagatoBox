' Gambas module file

Private $devices As New NagatoDevices

Public Sub GetStatus() As String
    
    Return $devices.Percentage
    
End

Public Function GetIcon() As Picture
    
    If $devices.Online Then
        Return MikuruIcon["plug"]
    Else
        Return MikuruIcon["battery"]
    Endif
    
End

Public Sub Activate()
    
    Print "Activate !!"
    
End
