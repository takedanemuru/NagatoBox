' Gambas class file

Inherits NagatoMenuButtons

Public Sub _SetButtons() 'override
    
    Dim yukiButton As Object
    
    With MikuruEvent
        yukiButton = New NagatoMenuButton(Me._$container, .ViewRotateLeft, "Rotate-Left", "button-rotate-ccw") As "Menu"
        yukiButton = New NagatoMenuButton(Me._$container, .ViewRotateRight, "Rotate-Right", "button-rotate-cw") As "Menu"
    End With
    
End
