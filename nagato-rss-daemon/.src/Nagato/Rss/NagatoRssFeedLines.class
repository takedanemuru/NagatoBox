' Gambas class file

Create Static

Private Const BBCTop As String = "http://feeds.bbci.co.uk/news/rss.xml"
Private Const BBCWorld As String = "http://feeds.bbci.co.uk/news/world/rss.xml"
Private Const BBCTechnology As String = "http://feeds.bbci.co.uk/news/technology/rss.xml"
Private Const ReutersTechnology As String = "http://feeds.reuters.com/reuters/technologyNews"
Private Const ReutersOddlyEnough As String = "http://feeds.reuters.com/reuters/oddlyEnoughNews"
Private Const Tass As String = "http://tass.com/rss/v2.xml"
Private Const France24France As String = "http://www.france24.com/en/france/rss"
Private Const Phoronix As String = "http://www.phoronix.com/rss.php"
Private Const LinuxComAll As String = "https://www.linux.com/feeds/rss"
Private Const LinuxComCommunityBlog As String = "https://www.linux.com/feeds/blogs/community/rss"
Private Const LinuxComDesktop As String = "https://www.linux.com/feeds/news/desktop/rss"
Private Const LinuxComTutorials As String = "https://www.linux.com/feeds/tutorials/rss"

Public Function GetDefault() As Collection
    
    Dim yukiLines As New Collection
    
    With yukiLines
        .Add(LinuxComAll, "Linux.com All")
        .Add(LinuxComCommunityBlog, "Linux.com Community Blog")
        .Add(LinuxComDesktop, "Linux.com Desktop")
        .Add(LinuxComTutorials, "Linux.com Tutorials")
        .Add(Phoronix, "Phoronix")
        .Add(ReutersTechnology, "Reuters Technology")
        .Add(ReutersOddlyEnough, "Reuters Oddly Enough")
        .Add(France24France, "France24 France")
        .Add(Tass, "TASS")
        .Add(BBCWorld, "BBC World")
        .Add(BBCTechnology, "BBC Technology")
    End With
    
    Return yukiLines
    
End
