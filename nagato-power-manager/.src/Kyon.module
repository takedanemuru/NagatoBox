' Gambas module file

Public Sub Main()
    
    NagatoArguments.Parsing()
    
    NagatoDBus.Register()
    
    NagatoUPower.EnsureActivation()
    
    If Not NagatoArguments.NonGui Then 
        If NagatoArguments.LogoutDialog Then
            HaruhiDialog.Show()
        Else
            HaruhiMain.Show()
        End If
    End If
    
End

Public Sub Quit()
    
    Quit(0)
    
End
