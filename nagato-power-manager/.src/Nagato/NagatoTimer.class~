Create Static

Private $timer As Timer

Public Sub Activate()
    
    $timer = New Timer As "Timer"
    $timer.Enabled = True
    
End

Public Sub Timer_Timer()
    
    Debug NagatoUPower.HasBattery
    Debug NagatoUPower.IsOnline
    Debug NagatoUPower.TotalBatteryPercentage
    
End