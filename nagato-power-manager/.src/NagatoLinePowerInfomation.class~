' Gambas class file

Inherits NagatoTabInfomation

Public Sub addContent() ' override.
        
        Me.$textArea.Text = "This is LinePower\n\n"
        Me.setInfomationText()
        Me.$drawingArea.Refresh()
        
End

Public Sub DrawingArea_Draw()
        
        Draw.Picture(MikuruBatteryIcon.GetPlugIcon(120), 10, 10)
        
End
