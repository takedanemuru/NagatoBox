' Gambas module file

Public Sub Main()

        Dim yukiSound As New Sound("sounds/welcumz.wav")
        Dim yukiArgs As String[]
        Dim yukiArg As String
        
        Args.Begin()
                yukiArg = Args.Get("p", "play", "play sound", "path to play")
        yukiArgs = Args.End()
        
        Debug yukiArg
        Debug yukiArgs.Count
        
        yukiSound.Play()

        Wait 30
        
        Quit

End
