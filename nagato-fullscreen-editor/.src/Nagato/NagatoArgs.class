' Gambas class file

Create Static

Property Read Path As String

Private $paths As New String[]

Public Sub Parse()
    
    Args.Begin("usage : ")
    
    $paths = Args.End()
    
End

Private Function Path_Read() As String

    Dim yukiPath As String
    
    For Each yukiPath In $paths
        If Exist(yukiPath) Then Return yukiPath
    Next

    Return ""

End
