' Gambas module file

Private $icon As NagatoTrayIcon

Public Sub Main()
    
    If Desktop.HasSystemTray Then
        NagatoAlsaState.Activate()
        HaruhiVolumeDialog.Activate()
        $icon = New NagatoTrayIcon
    Else
        Message.Error(("YUKI.N > No systray"), ("OK"))
    End If
    
End
