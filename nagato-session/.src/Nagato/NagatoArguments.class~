' Gambas class file

Create Static

Property Read Autostart As Boolean
Property Read Logout As Boolean

Private $autostart As Boolean = False
Private $logout As Boolean = False

Public Sub Parsing()
    
    Args.Begin("Usage")
        $autostart = Args.Has("a", "autostart", "execute xdg autostart")
        $logout = Args.Has("l", "logout", "show logout screen")
    Args.End()
    
End

Private Function Autostart_Read() As Boolean

    Return $autostart

End
