Inherits NagatoUXContainerVirtical

Private $execLabel As NagatoUXDesktopEntryExec
Private $tryExecLabel As NagatoUXDesktopEntryExec

Public Sub _OnInitializeWidgets()
    
    $execLabel = New NagatoUXDesktopEntryExec(Me._$container, "Exec")
    $tryExecLabel = New NagatoUXDesktopEntryExec(Me._$container, "Try Exec")
    
End

Public Sub SetProperties(argAutostartFile As NagatoAutostartFile)
    
    $execLabel.SetProperty(argAutostartFile.Exec)
    $tryExecLabel.SetProperty(argAutostartFile.TryExec)
    
End