' Gambas class file

Create Static

Public Enum None, Choir, Ubuntu, Welcumz, UserDefined

Public Sub PlayDefault()
    
    Select Case NagatoSettings.LoginSoundMode
        Case Me.None
            Return
        Case Me.Choir
            Sound.Load("sounds/choir.wav").Play()
        Case Me.Welcumz
            Sound.Load("sounds/welcumz.wav").Play()
        Case Me.Ubuntu
            Sound.Load("sounds/ubuntu.wav").Play()
    End Select

End

