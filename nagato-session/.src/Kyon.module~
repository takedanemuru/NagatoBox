' Gambas module file

Public Sub Main()
    
    NagatoArguments.Parsing()
    
    If Not NagatoArguments.Autostart Then
        HaruhiMain.Show()
    Else
        HaruhiWelcome.Show()
        NagatoLoginSound.PlayDefault()
        NagatoXdgAutostarts.Execute()
        HaruhiWelcome.Close()
    End If
    
End
