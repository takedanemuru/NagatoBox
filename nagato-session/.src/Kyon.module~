' Gambas module file


Public Sub Main()
    
    NagatoArguments.Parsing()
    
    If NagatoArguments.Autostart Then
        NagatoLoginSound.PlayDefault()
        NagatoXdgAutostarts.Execute()
    Else If NagatoArguments.Logout Then
        HaruhiDialog.Show()
    Else
        HaruhiMain.Show()
    End If
    
End
