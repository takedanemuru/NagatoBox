' Gambas class file

Private $exec As NagatoUXContainerDesktopEntry
Private $onlyShowIn As NagatoUXContainerListEditor
Private $notShowIn As NagatoUXContainerListEditor

Public Sub SetProperties(argAutostartFile As NagatoAutostartFile)
    
    $exec.SetProperties(argAutostartFile)
    $onlyShowIn.SetList(argAutostartFile.OnlyShowIn)
    $notShowIn.SetList(argAutostartFile.NotShowIn)
    
End

Public Sub _new()
    
    $exec = New NagatoUXContainerDesktopEntry(MainVBox)
    $onlyShowIn = New NagatoUXContainerListEditor(MainVBox, "Only Show In...")
    $notShowIn = New NagatoUXContainerListEditor(MainVBox, "Not Show In...")
    
End
