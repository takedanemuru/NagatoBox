' Gambas class file


Public Sub Form_Open()

    Me.Picture = Image.Load("pictures/yukichan_28.jpg").Stretch(Me.W, Me.H).Picture
    Me.Center()

End

Public Sub CancelButton_Click()

    Me.Close(MikuruDialog.ResponseCancel)

End

Public Sub QuitButton_Click()

    Me.Close(MikuruDialog.ResponseQuit)

End
