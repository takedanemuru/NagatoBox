' Gambas class file

Private $settings As New Settings

Public Sub _new()

End

Public Sub Form_Open()

    Me.X = $settings["fmain/x", Me.X]
    Me.Y = $settings["fmain/y", Me.Y]

End

Public Sub Form_Close()
    
    $settings["fmain/x"] = Me.X
    $settings["fmain/y"] = Me.Y
    
End


Public Sub Form_KeyPress()
    
    Label1.Text = Subst$("  keycode: &1", Key.Code)
    
End

Public Sub Label1_MouseDown()

    

End
