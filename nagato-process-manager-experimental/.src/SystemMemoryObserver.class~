' Gambas class file

Private $memoryData As String
Private $memoryDataArray As String[]

Event ObservationFinished(argRate As Float)

Private Sub observeMemory()
    
    Dim yukiMemoryTotal As String
    Dim yukiMemoryActive As String
    Dim yukiMemoryActiveAnon As String
    Dim result As Float
    
    $memoryData = File.Load("/proc/meminfo")
    $memoryDataArray = Split($memoryData, "\n", "")
   
   Raise ObservationFinished(0.1)
    
End

Public Sub _new()
    
    observeMemory()
    
End
