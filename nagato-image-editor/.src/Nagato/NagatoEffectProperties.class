Inherits NagatoMenuPipe

Property Read Values As Variant[]

Public _$container As Container

Public Sub _AddLabel(argText As String)
    
    Dim yukiLabel As New Label(Me._$container)
    
    With yukiLabel
        .Font.Bold = True
        .Foreground = Color.White
        .Alignment = Align.Center
        .H = 32
        .Text = argText
    End With
    
End

Private Function Values_Read() As Variant[]

    Return [""]

End
