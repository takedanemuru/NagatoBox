' Gambas class file

Create Static

Public Const BusName As String = "org.gambas.dataovermind-systemmonitor"
Public Const ObjectPath As String = "/org/dataovermind/systemmonitor"
Public Const Interface As String = "org.gambas.dataovermind.systemmonitor.tfei"

Public Function Activated() As Boolean
    
    Return DBus.Session.Applications.Exist(Me.BusName)
    
End

Public Function IsUnique() As Boolean
    
    Return Not DBus.Session.Applications.Exist(Me.BusName)
    
End

Public Sub Register()
    
    DBus.Name = Me.BusName
    DBus.Session.Register(TFEI, Me.ObjectPath)
    
End

Public Sub Unregister()
    
    DBus.Session.Unregister(TFEI)
    
End
