' Gambas class file

Create Static

Property Read Proxy As DBusProxy

Public Const Service As String = "org.gambas.dataovermind.systemmonitor"
Public Const ObjectPath As String = "/org/dataovermind/systemmonitor"
Public Const Interface As String = "org.gambas.dataovermind.systemmonitor.tfei"

Public Function Activated() As Boolean
    
    Return DBus.Session.Applications.Exist(Me.Service)
    
End

Public Function IsUnique() As Boolean
    
    Return Not DBus.Session.Applications.Exist(Me.Service)
    
End

Public Sub Register()
    
    DBus.Session.Register(TFEI, Me.ObjectPath)
    
End

Public Sub Unregister()
    
    DBus.Session.Unregister(TFEI)
    
End

Private Function Proxy_Read() As DBusProxy

    Return DBus[Me.Service][Me.ObjectPath, Me.Interface]

End
