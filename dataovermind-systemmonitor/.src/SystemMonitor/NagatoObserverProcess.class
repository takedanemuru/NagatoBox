' Gambas class file

Inherits NagatoObserver

Private Function getProcessCount() As Integer
    
    Return Dir("/proc", "[0-9]*", gb.Directory).Count
    
End

Public Sub GetStep() ' virtual
    
    Dim yukiUsage As Integer = getProcessCount()
    
    Me._$usage = yukiUsage
    Me._$history.Push(yukiUsage)
    
End