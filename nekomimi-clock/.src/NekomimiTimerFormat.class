Create Static

Public Function getHourPart(argSeconds As Integer) As Integer
    
    Return (argSeconds Div 3600) Mod 100
    
End

Public Function getMinutePart(argSeconds As Integer) As Integer
    
    Return (argSeconds Div 60) Mod 60
    
End

Public Function getSecondPart(argSeconds As Integer) As Integer
    
    Return argSeconds Mod 60
    
End
