' Gambas class file

Inherits NekomimiEngineCore

Public Sub _setProperties()
    
    Dim nekomimiNow As Date = Now
    
    Me.$hourTen = Hour(nekomimiNow) Div 10
    Me.$hourOne = Hour(nekomimiNow) Mod 10
    Me.$MinuteTen = Minute(nekomimiNow) Div 10
    Me.$MinuteOne = Minute(nekomimiNow) Mod 10
    Me.$secondTen = Second(nekomimiNow) Div 10
    Me.$secondOne = Second(nekomimiNow) Mod 10
    
End
