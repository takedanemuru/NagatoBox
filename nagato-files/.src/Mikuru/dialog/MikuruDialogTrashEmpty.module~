' Gambas module file

Public Sub _call()
    
    Dim yukiMessage As String = ("YUKI.N > We are going to empty trash bin...")
    
    yukiMessage &= Subst$("&1\n&2 items &3", Space$(8), NagatoTrash.Count, NagatoTrash.Size)
    
    If Message.Question(yukiMessage, ("Cancel"), ("Empty")) = 2 Then
        Shell "trash-empty"
    Endif
    
End

