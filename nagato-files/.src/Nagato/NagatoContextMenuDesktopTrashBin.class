' Gambas class file

Inherits NagatoContextMenu

Public Sub _ClickAction()
    
    Select Case Last.Tag
        Case 0
            Try Shell "nagato-files -t"
        Case 1
            Try Shell "gvfs-trash --empty"
    End Select
    
End

Public Sub _AddChildMenus()
    
    Me._AddContextMenu(("Open"), 0, MikuruIcon.Get("folder"))
    Me._AddContextMenu(("Empty"), 1, MikuruIcon.Get("bin"))
        
End
