' Gambas class file

Property Read IconView As IconView

Private $iconView As IconView

Public Sub _new(argContainer As Container)
    
    $iconView = New IconView(argContainer)
    
    With $iconView
        .Expand = True
        .Orientation = Arrange.Horizontal
        .Border = False
        .Mode = Select.Single
        .Drop = True
    End With
    
End

Private Function IconView_Read() As IconView

    Return $iconView

End
