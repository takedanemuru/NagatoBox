' Gambas class file

Inherits NagatoSelectionBarWidget

Private $list As ComboBox

Public Sub _Initialize()
    
    Dim yukiPath As String
    
    $list = New ComboBox(Me._$container) As "WhatList"
    
    With $list
        .ReadOnly = True
        .List = ["ClipBoard", "Home", "Dialog"]
        .W = 160
        For Each yukiPath In NagatoXdg.UserDirs.All
            .Add(File.Name(yukiPath))
        Next
    End With
    
End
