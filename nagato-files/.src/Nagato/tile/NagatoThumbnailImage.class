Inherits NagatoThumbnail

Public Function _GetIconImage(argPath As String) As Image
    
    Dim yukiImage As Image = Image.Load(argPath)
    
    Return yukiImage
    
Catch
    yukiImage = New Image(Me.ThumbnailSize, Me.ThumbnailSize, Me._GetLabelColor())
    Return yukiImage
    
End

Public Function _GetLabelColor() As Integer 'override
    
    Return Color.Violet
    
End
