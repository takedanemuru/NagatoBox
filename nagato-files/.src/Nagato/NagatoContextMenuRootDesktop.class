' Gambas class file

Inherits NagatoContextMenu

Private Sub addSortMenu()
    
    Dim yukiMenu As NagatoSortMenuDesktop
    
    yukiMenu = New NagatoSortMenuDesktop(Me._$rootMenu) As "Menu"
    
End

Public Sub _AddChildMenus()
    
    With NagatoMenuPipe
        Object.New("NagatoMenuItemChangeWallpaper", [Me._$rootMenu, ""])
        Object.New("NagatoMenuItemCreateLauncher", [Me._$rootMenu, ""])
        addSortMenu()
    End With
    
End
