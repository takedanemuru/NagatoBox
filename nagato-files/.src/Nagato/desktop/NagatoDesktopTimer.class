' Gambas class file

Create Static

Private $timer As Timer

Event Timer

Public Sub Stop()
    
    $timer.Stop()
    
End

Public Sub Start()
    
    $timer = New Timer As "Timer"
    
    With $timer
        .Delay = 1000
        .Enabled = True
    End With
    
End

Public Sub Timer_Timer()
     
     Raise Timer
    
End
