Inherits NagatoSelectionBarWidget

Private $cancelButton As MenuButton

Public Sub _Initialize()
    
    $cancelButton = New MenuButton(Me._$container) As "CancelButton"
    
    With $cancelButton
        .Text = ("Cancel")
        .AutoResize = True
        .Border = True
        .Arrow = False
    End With
    
End