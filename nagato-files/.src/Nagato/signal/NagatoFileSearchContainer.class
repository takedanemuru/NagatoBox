Inherits NagatoLeftPaneContent

Property Read VBox As VBox

Private $vBox As VBox

Public Sub _new(argContainer As Container)
    
    $vBox = New VBox(argContainer)
    
    With $vBox
        .Expand = True
        .Background = Color.Lighter(Color.Green)
        .Visible = False
    End With
    
End

Private Function VBox_Read() As VBox

    Return $vBox

End
