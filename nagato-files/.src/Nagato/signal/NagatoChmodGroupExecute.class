Public Sub _SetRadioButtons() ' virtual interface
    
    Me._SetRadioButton("-", "- : The file group cannot execute it.")
    Me._SetRadioButton("x", "x : The file group can execute it.")
    Me._SetRadioButton("S", "S : The file group cannot execute it and the 'setgid' bit is set.")
    Me._SetRadioButton("s", "s : The file group can execute it and the 'setgid' bit is set.")
    
End
