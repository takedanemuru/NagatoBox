' Gambas class file

Create Private

Inherits NagatoLeftPaneContent

Public _$listView As ListView
Public _$observer As Observer

Public Sub _new(argListView As ListView)
    
    Me._$listView = argListView
    Me._$listView.SetFocus()
    Me._$observer = New Observer(Me._$listView) As "Asakura"
    
End

