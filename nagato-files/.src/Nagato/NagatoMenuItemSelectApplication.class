' Gambas class file

Inherits NagatoMenuItem

Public Sub _InitializeItem()
    
    Dim yukiMenu As New Menu(Me._$parentMenu) As "MenuItem"
    
    With yukiMenu
        .Text = ("Select application")
        .Picture = MikuruIcon.Get("gear")
    End With
    
End

Public Sub MenuItem_Click()
    
    HaruhiSelectApplication.ShowAllApplications(Me._$path)
    
End
