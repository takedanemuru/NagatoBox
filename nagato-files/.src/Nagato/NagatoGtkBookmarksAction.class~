' Gambas class file

Inherits NagatoGtkBookmarksHandler

Public Sub Asakura_Activate()
    
    Debug Me._$listView.Key
    
    Select Case Me._$listView.Key
        Case "NagatoTrashBinKey"
            Debug "I came here."
            HaruhiMain.SetNewTrashBin()
        Default
            HaruhiMain.SetNewDirectory(Me._$listView.Key)
    End Select
    
End
