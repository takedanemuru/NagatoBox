' Gambas class file

Inherits NagatoMenuItem

Public Sub _InitializeItem() 'override
    
    Dim yukiMenu As New Menu(Me._$parentMenu) As "ExecuteMenu"
    
    With yukiMenu
        .Text = ("Execute")
        .Picture = MikuruIcon.Get("gear")
        .Tag = Me._$path
    End With
    
End

Public Sub ExecuteMenu_Click()
    
    Dim yukiDesktop As New DesktopFile(Last.Tag)
    
    yukiDesktop.Run("")
    
End
