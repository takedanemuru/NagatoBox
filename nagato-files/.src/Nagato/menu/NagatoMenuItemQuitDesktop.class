' Gambas class file

Inherits NagatoMenuItem

Public Sub _InitializeItem() 'override
    
    Dim yukiMenu As New Menu(Me._$parentMenu) As "MenuItem"
    
    With yukiMenu
        .Text = ("Quit Desktop")
        .Picture = MikuruIcon.Get("button-power")
        .Tag = Me._$path
    End With
    
End

Public Sub MenuItem_Click()
    
    HaruhiDesktop.Close()
    
End
