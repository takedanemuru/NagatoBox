' Gambas class file

Inherits NagatoMenuItem

Public Sub _InitializeItem() 'override
    
    Dim yukiMenu As New Menu(Me._$parentMenu) As "MenuItem"
    
    With yukiMenu
        .Text = ("Show Properties")
        .Picture = MikuruIcon.Get("information-balloon")
        .Tag = Me._$path
    End With
    
End

Public Sub MenuItem_Click()
    
    HaruhiProperty.SetProperty(Me._$path)
    HaruhiProperty.ShowDialog()
    
End
