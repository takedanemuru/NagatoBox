' Gambas class file

Inherits NagatoMenuItem

Public Sub _InitializeItem() 'override
    
    Dim yukiMenu As New Menu(Me._$parentMenu) As "MenuItem"
    
    With yukiMenu
        .Text = ("Show Music")
        .Picture = MikuruIcon.GetCheckButton(NagatoSettingsDesktopTile.ShowMusic)
        .Tag = Me._$path
    End With
    
End

Public Sub MenuItem_Click()
    
    NagatoSettingsDesktopTile.ShowMusic = Not NagatoSettingsDesktopTile.ShowMusic
    
End
