' Gambas class file

Inherits NagatoMenuItem

Public Sub _InitializeItem()
    
    Dim yukiMenu As New Menu(Me._$parentMenu) As "MenuItem"
    
    With yukiMenu
        .Text = ("Rename")
        .Picture = MikuruIcon.Get("button-rotate-cw")
    End With
    
End

Public Sub MenuItem_Click()
    
    If HaruhiRename.SetPath(Me._$path, Desktop.GetFileIcon(Me._$path, 64)) Then
        HaruhiRename.ShowDialog()
    End If
    
End
