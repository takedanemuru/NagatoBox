Inherits NagatoMenuPanelWidget

Public Sub _Initialize()
    
    Dim yukiButton As New MenuButton(Me._$container)
    Dim yukiMenu As NagatoStarMenu
    
    With yukiButton
        .Arrow = False
        .AutoResize = True
        .Picture = MikuruIcon.Get("star")
        .Border = False
    End With
    
    yukiMenu = New NagatoStarMenu(HaruhiMain, yukiButton, "MenuIndex" & MikuruUniqueIndex.Get()) As "TFEI"
    
End
