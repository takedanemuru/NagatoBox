' Gambas module file

Public Sub _get(argFilePath As String, argHeader As String) As String
    
    Dim yukiLine As String
    
    For Each yukiLine In Split(File.Load(argFilePath), "\n", "", True)
        If yukiLine Begins argHeader & "=" Then Return Replace(yukiLine, argHeader & "=", "")
    Next
    
Finally
    Return ""
    
End

