' Gambas class file

Inherits NagatoMenuButton

Private Const MenuName As String = "BookmarkMenu_"

Private Function getMenuName() As String
    
    Return MenuName & Me._$uniqueName
    
End

Public Sub _OnInitializeButton()
    
    With Me._$button
        .Picture = MikuruIcon["book-open-bookmark"]
        .Tooltip = ("Bookmarks")
        .Menu = getMenuName()
    End With
    
End

Public Sub _OptionalOnInitializePulldownMenu()
    
    Dim yukiMenu As NagatoPulldownMenuBookmark
    
    yukiMenu = New NagatoPulldownMenuBookmark(getMenuName(), Me._$uniqueName) As "TFEI"
    
End
