' Gambas class file

Inherits NagatoMenuButton

Private MenuName As String = "StarMenu_"

Private Function getMenuName() As String
    
    Return MenuName & Me._$uniqueName
    
End

Public Sub _OnInitializeButton()
    
    With Me._$button
        .Picture = MikuruIcon["star"]
        .Tooltip = ("Main Menu")
        .Menu = getMenuName()
    End With
    
End

Public Sub _OptionalOnInitializePulldownMenu()
    
    Dim yukiMenu As NagatoPulldownMenuStar
    
    yukiMenu = New NagatoPulldownMenuStar(getMenuName(), Me._$uniqueName) As "TFEI"
    
End
