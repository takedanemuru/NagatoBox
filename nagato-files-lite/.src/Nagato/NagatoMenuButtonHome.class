Inherits NagatoMenuButton

Public Sub _OnInitializeButton()
    
    With Me._$button
        .Picture = MikuruIcon["home"]
        .Tooltip = ("home")
        .Menu = "do not append pulldown menu"
    End With
    
End

Public Sub _OnClickMenuButton()
    
    Raise Signal(MikuruSignal.UserInputMoveToDirectory, [User.Home])
    
End
