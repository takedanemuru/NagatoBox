' Gambas class file

Create Static

Property Read Paths As String[]

Private $arguments As String[]

Public Sub Parsing()
    
    Dim yukiOption As Boolean
    
    Args.Begin("nagato-archiver usage")
        yukiOption = Args.Get("e", "extract-files-here ", " Extract files In current directory ")
    $arguments = Args.End()
    
    If $arguments.Count > 0 Then NagatoFileStore.SetPaths($arguments)

End

Private Function Paths_Read() As String[]

    Return $arguments

End
