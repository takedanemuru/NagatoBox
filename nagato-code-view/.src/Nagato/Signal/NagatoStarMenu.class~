' Gambas class file

Inherits NagatoButtonMenu

Public _$uniqueName As String = "StarMenu"

Public Sub _RefreshChildMenus() ' override
    
    Me._AddMenu("Open", MikuruSignal.ViewPreview, "search")
    'Object.New("NagatoSubMenuSettings", [Me._$parentMenu, ("Settings"), "equalizer"])
        Me._AddMenu("", 0)
    Me._AddMenu("About", MikuruSignal.AppAbout, "star")
    Me._AddMenu("Quit", MikuruSignal.AppQuit, "button-power")
    
End
