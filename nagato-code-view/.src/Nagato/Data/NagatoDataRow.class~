' Gambas class file

Inherits NagatoSignalData

Property Read Collection As Collection

Private $collection As New Collection

Public Sub _new(argType As String, argPath As String) 
    
    With $collection
        .[MikuruColumn.KeyType] = argType
        .[MikuruColumn.KeyName] = File.Name(argPath)
        .[MikuruColumn.KeyLines] = $reader.ReadLine(argPath)
        .[MikuruColumn.KeyLinesWithoutVoid] = $reader.ReadLineWithoutVoid(argPath)
    End With
    
End

Private Function Collection_Read() As Collection

    Return $collection

End
