' Gambas module file

Property Read Desktop As String
Property Read Pictures As String

Private Function Desktop_Read() As String

    Dim yukiReturn As String
    
    Shell "xdg-user-dir DESKTOP" To yukiReturn
    
    Return Replace$(yukiReturn, "\n", "")

End

Private Function Pictures_Read() As String

    Dim yukiReturn As String
    
    Shell "xdg-user-dir PICTURES" To yukiReturn
    
    Return Replace$(yukiReturn, "\n", "")

End
