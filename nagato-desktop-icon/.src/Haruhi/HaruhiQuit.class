' Gambas class file

Public Enum ResponseCancel = 1001, ResponseQuit = 1002

Public Sub Form_Open()

    Dim yukiImage As Image = Image.Load("pictures/yukichan_23.jpg").Stretch(Me.W, Me.H)

    Me.Picture = yukiImage.Picture
    Me.Center()

End

Public Sub CancelButton_Click()

    Me.Close(Me.ResponseCancel)

End


Public Sub QuitButton_Click()
    
    NagatoDBus.Unregister()
    
    Shell Subst$("nagato-taskmanager --unregister &1", Application.Name)
    
    Me.Close(Me.ResponseQuit)

End
