Inherits NagatoThumbnail

Public Function _GetUnresizedThumbnail() As Image
    
    Dim yukiImage As Image = Image.Load(NagatoPreviewProperties.Path)
    Dim yukiRatio As Float = HaruhiMain.PreviewRatio
    
    Return yukiImage.Stretch(yukiImage.Width * yukiRatio, yukiImage.Height * yukiRatio)
    
End
