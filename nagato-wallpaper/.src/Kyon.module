Public Sub Main()
    
    Dim yukiArguments As New NagatoArguments
    
    MikuruHidden.Ensure()
    
    If yukiArguments.SetLast Then
        NagatoWallpaperSetter.SetLast()
    Else
        HaruhiMain.Show()
        If yukiArguments.Options.Count > 0 Then HaruhiMain.SetPath(yukiArguments.Options[0])
    End If
    
End
