' Gambas class file

Create Static

Private $proxy As DBusProxy

Public Sub AddTab(argDirectory As String)
    
    $proxy.add_tab(argDirectory)
    $proxy.show_hide()
    
End

Public Sub _new()
    
    $proxy = DBus["Session://org.guake.RemoteControl"]["/org/guake/RemoteControl", "org.guake.RemoteControl"]
    
End
