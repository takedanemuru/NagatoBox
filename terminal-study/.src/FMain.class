Public Sub Form_Open()

    Dim yukiWindow As DesktopWindow

    Shell Subst$("xterm -into &1 +u8", Embedder1.Id) For Read As "TerminalProcess"
    
End

Public Sub Form_Close()
    
    Dim yukiResponse As String
    
    Shell "pgrep xterm" To yukiResponse
    Shell Subst$("kill &1", yukiResponse)
    
End

