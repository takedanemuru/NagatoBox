' Gambas module file

Public Function GetImage(argIndex As Integer, Optional argSize As Integer) As Image
    
    Dim yukiImage As Image = Image.Load(Subst$("clockface/clock&1.png", argIndex))
    
    If argSize Then yukiImage = yukiImage.Stretch(argSize, argSize)
    
    Return yukiImage
    
Catch
    Return Null
    
End
