' Gambas class file

Public Sub Form_Open()
    
    Me.Center()
    Me.Picture = Image.Load("pictures/yukichan_36.jpg").Stretch(Me.Width, Me.Height).Picture
    
End



Public Sub SuperUserButton_Click()

    Me.Close(9)

End

Public Sub NormalUserButton_Click()

    Me.Close(8)

End

Public Sub CancelButton_Click()

    Me.Close(0)

End
