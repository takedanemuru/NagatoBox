Public Sub Form_Open()

    Dim yukiCounter As Integer
    
    For yukiCounter = 1 To 28
        IconView1.Add(yukiCounter, yukiCounter, MikuruClockFace.GetImage(yukiCounter, 144).Picture)
    Next

End

Public Sub IconView1_Activate()
    
    Me.Close(IconView1.Key)
    
End
