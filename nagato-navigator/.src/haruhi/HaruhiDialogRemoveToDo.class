Public Enum ResponseCancel, ResponseRemove

Public Sub Form_Open()

    Me.Center()
    Me.Picture = Image.Load("pictures/yukichan_37.jpg").Stretch(Me.W, Me.H).Picture

End

Public Sub RemoveButton_Click()

    Me.Close(ResponseRemove)

End

Public Sub CancelButton_Click()

    Me.Close(ResponseCancel)

End
