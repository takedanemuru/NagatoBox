' Gambas class file

Public _$menuButton As MenuButton
Public _$dummyRootMenu As Menu
Public _$uniqueName As String = "MenuButtonHandlerParent"

Public Sub _AddMenu() ' This method is virtual, MUST be overridden.
    
    
    
End

Public Sub _new(argMenuButton As MenuButton)
    
    Me._$dummyRootMenu = New Menu(Kyon)
    Me._$dummyRootMenu.Name = Me._$uniqueName

    Me._$menuButton = argMenuButton
    Me._$menuButton.Menu = Me._$uniqueName
    
    Me._AddMenu()
    
End
