' Gambas class file

Property Read ProcessId As Integer

Private $process As Process
Private $embedder As Embedder

Public Sub _new(argContainer As Container)
    
    $embedder = New Embedder(argContainer) 
    $embedder.Expand = True
    
    $process = Exec ["xterm", "-into", $embedder.Id, "-fg", "green", "-bg", "black"] 
    
End

Private Function ProcessId_Read() As Integer

    Return $process.Id

End
