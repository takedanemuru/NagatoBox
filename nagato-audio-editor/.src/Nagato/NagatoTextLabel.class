' Gambas class file

Inherits NagatoObject

Private $textLabel As TextLabel

Public Sub _new(argContainer As Container)
    
    $textLabel = New TextLabel(argContainer) As "TextArea"
    
    With $textLabel
        .Expand = True
        .Alignment = Align.Left
        .Text = ("YUKI.N > Open file to edit.")
    End With
    
End
