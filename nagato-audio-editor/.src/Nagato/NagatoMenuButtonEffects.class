' Gambas class file

Inherits NagatoMenuButton

Private Const MenuName As String = "EffectsMenu"

Private $pulldownMenu As NagatoPullDownMenuEffects

Public Sub _OnInitializeMenuButton(argContainer As Container)
    
    With Me._$menuButton
        .Picture = MikuruIcon["gear"]
        .Menu = MenuName
    End With
    
End

Public Sub _OptionalOnInitializePulldownMenu(argContainer As Container)
    
    $pulldownMenu = New NagatoPullDownMenuEffects(MenuName)
    
End
