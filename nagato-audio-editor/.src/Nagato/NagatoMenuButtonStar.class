' Gambas class file

Inherits NagatoMenuButton

Private Const MenuName As String = "StarMenu"

Private $pulldownMenu As NagatoPullDownMenuStar

Public Sub _OnInitializeMenuButton(argContainer As Container)
    
    With Me._$menuButton
        .Picture = MikuruIcon["star"]
        .Menu = MenuName
    End With
    
End

Public Sub _OptionalOnInitializePulldownMenu(argContainer As Container)
    
    $pulldownMenu = New NagatoPullDownMenuStar(MenuName)
    
End