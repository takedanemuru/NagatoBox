' Gambas module file

Private Function IsUnique() As Boolean
    
    If Not DBus.Session.Applications.Exist("org.gambas.nagato.lookup") Then
        NagatoDBus.Register()
        Return True
    End If
    
End

Public Sub Main()
    
    If IsUnique() Then
        NagatoMenuCache3.Activate()
        HaruhiMain.Show()
    End If
    
End
