' Gambas module file

Private Function IsUnique() As Boolean
    
    Dim yukiWindow As DesktopWindow
    
    For Each yukiWindow In Desktop.Windows
        If yukiWindow.VisibleName = Application.Name Then Return False
    Next
    
    Return True
    
End

Public Sub Main()
    
    If IsUnique Then
        NagatoMenuCache3.Activate()
        HaruhiMain.Show()
    End If
    
End

