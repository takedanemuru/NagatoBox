' Gambas class file

Public Enum None, NormalUser, SuperUser

Public Sub Form_Open()
    
    Me.Center()
    Me.Picture = Image.Load("pictures/dialog_300.jpg").Stretch(Me.Width, Me.Height).Picture
    
End

Public Sub SuperUserButton_Click()

    Me.Close(Me.SuperUser)

End

Public Sub NormalUserButton_Click()

    Me.Close(Me.NormalUser)

End

Public Sub CancelButton_Click()

    Me.Close(Me.None)

End
