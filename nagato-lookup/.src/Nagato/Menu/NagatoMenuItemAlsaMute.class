Inherits NagatoMenuItemAlsa

Public Sub _AbstractOnInitialize(argParentMenu As Menu)
    
    Me._$menu = New Menu(argParentMenu) As "MenuItem"
    
    With Me._$menu
        .Text = ("Mute")
        .Picture = MikuruIcons.Get("speaker-mute")
    End With
    
End

Public Sub _OptionalAbstractOnInitialize(argValues As Variant[])
    
    Me._$channel = argValues[0]
    Me._$menu.Tag = argValues[1]
    
End
