' Gambas class file

Inherits NagatoWidget

Private Const MenuName As String = "StarMenu"

Public Sub _InitializeWidget() 'override
    
    Dim yukiButton As New MenuButton(Me._$container)
    Dim yukiMenu As NagatoPullDownMenuStar
    
    With yukiButton
        .Picture = MikuruIcons.Get("star")
        .Width = Me._$container.Height
        .Border = False
        .Menu = MenuName
    End With
    
    yukiMenu = New NagatoPullDownMenuStar(MenuName)
    
End
