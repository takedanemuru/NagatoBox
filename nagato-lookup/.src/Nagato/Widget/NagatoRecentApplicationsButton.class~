' Gambas class file

Inherits NagatoWidget

Private Const MenuName As String = "RecentApplications"

Private $menuButton As NagatoMenuButton
Private $pulldownMenu As NagatoPullDownMenuRecent

Public Sub _InitializeWidget() 'override
    
    $menuButton = New NagatoMenuButton(Me._$container, ("Recent"), "alarm-clock", MenuName)
    $pulldownMenu = New NagatoPullDownMenuRecent(MenuName) As "TFEI"
    
End
