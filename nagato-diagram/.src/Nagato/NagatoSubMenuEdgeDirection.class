' Gambas class file

Inherits NagatoSubMenu

Public Sub _OnGetParentMenuName() As String
    
    Return ("Direction")
    
End

Public Sub _OnInitializeChildMenus()
    
    Dim yukiStyle As String
    
    For Each yukiStyle In ["forward", "back", "both", "none"].sort()
        Me._AddMenuItem(yukiStyle, "dir = " & Quote(yukiStyle))
    Next
    
End
