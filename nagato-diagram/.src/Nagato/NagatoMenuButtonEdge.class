Inherits NagatoMenuButton

Private Const MenuName As String = "EdgeMenu"

Private $pulldownMenu As NagatoPullDownMenuEdge

Public Sub _OnInitializeButton()
    
    Me._AddButton("Edge Attributes", MikuruSignal.Void, "navigation-right-button")
    
End

Public Sub _OptionalOnInitializePullDownMenu()
    
    Me._$button.Menu = MenuName
    $pulldownMenu = New NagatoPullDownMenuEdge(MenuName) As "TFEI"
    
End
