' Gambas module file

Private $player As New MediaPlayer

Public Sub _call()
    
    Dim yukiPath As String
    
    For Each yukiPath In NagatoSettingsMusic.LoadPaths()
        $player.URL = Media.URL(yukiPath)
        Debug $player.Duration
    Next
    
    $player.Close()
    
End

Public Sub Player_Duration()
    
    Debug "x"
    
End
