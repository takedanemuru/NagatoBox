Public Function SetNewMusic(argCount As Integer) As Boolean
    
    If argCount = 1 Then
        HaruhiPlayer.SetMessage("<b>1 music is added</b>", 5)
    Else If argCount > 1 Then
        HaruhiPlayer.SetMessage(Subst$("<b>&1 musics are added</b>", argCount), 5)
    End If
    
    Return (argCount > 0)
    
End
