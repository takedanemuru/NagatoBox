' Gambas class file

Inherits NagatoMusicListAssistant

Property Read Url As String
Property Read Path As String
Property Read Position As Integer

Private Function Path_Read() As String

    Return Me._$musicElements[Me._$position.Current].Path

Catch
    Return ""

End

Private Function Url_Read() As String

    Return Media.URL(Me._$musicElements[Me._$position.Current].Path)

Catch
    Return ""

End

Private Function Position_Read() As Integer

    Return Me._$position.Current

End
