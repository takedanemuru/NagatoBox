Inherits NagatoMusicListAssistant

Public Function Get(argTagType As Integer) As String
    
    If Me._$musicElements.Count = 0 Then Return ""
    
    Return Me._$musicElements.GetTag(argTagType, Me._$position.Current)
    
End

Public Sub Set(argTitle As String, argArtist As String, argAlbum As String) 
    
    Me._$musicElements.SetTag(Me._$position.Current, argTitle, argArtist, argAlbum)
    
End
