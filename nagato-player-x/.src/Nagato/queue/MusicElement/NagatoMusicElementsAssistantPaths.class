' Gambas class file

Property Read Count As Integer
Property Read Max As Integer
Property Read All As String[]

Private $musicElements As NagatoMusicElement[]

Public Sub _new(argElements As NagatoMusicElement[])
    
    $musicElements = argElements
    
End

Private Function Count_Read() As Integer

    Return $musicElements.Count

End

Private Function All_Read() As String[]

    Dim yukiPaths As New String[]
    Dim yukiCount As Integer
    
    For yukiCount = 0 To $musicElements.Count - 1
        yukiPaths.Add($musicElements[yukiCount].Path)
    Next

    Return yukiPaths

End

Private Function Max_Read() As Integer

    Return $musicElements.Max

End
