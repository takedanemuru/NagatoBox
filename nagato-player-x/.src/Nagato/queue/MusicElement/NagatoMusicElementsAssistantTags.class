Private $musicElements As NagatoMusicElement[]

Public Function Get(argTag As Integer, argIndex As Integer) As String
    
    Select Case argTag
        Case MikuruTag.Title
            Return $musicElements[argIndex].Title
        Case MikuruTag.Artist
            Return $musicElements[argIndex].Artist
        Case MikuruTag.Album
            Return $musicElements[argIndex].Album
    End Select
    
Catch
    Return ""
    
End

Public Sub Set(argIndex As Integer, argTitle As String, argArtist As String, argAlbum As String) 
    
    $musicElements[argIndex].Title = argTitle
    $musicElements[argIndex].Artist = argArtist
    $musicElements[argIndex].Album = argAlbum
    
Catch
    Return
    
End

Public Sub _new(argElements As NagatoMusicElement[])
    
    $musicElements = argElements
    
End
