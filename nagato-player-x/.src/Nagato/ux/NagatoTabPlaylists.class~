' Gambas class file

Inherits NagatoTab

Private $menuBar As NagatoMenuBarPlaylists
Private $grids As NagatoGridsPlaylists

Public Sub Refresh()
    
    $grids.Refresh()
    
End

Public Sub _OnInitialize(argContainer As Container, argGrids As GridView)
    
    $menuBar = New NagatoMenuBarPlaylists(argContainer) As "MenuBar"
    $grids = New NagatoGridsPlaylists(argGrids) As "Grids"
    
End
