' Gambas class file

Inherits NagatoSignal

Private $grids As NagatoGridsPlaylists

Private Sub initializeStarMenu(argButton As MenuButton)
    
    Dim yukiMenu As New NagatoPlaylistsMenu As "StarMenu"
    
    argButton.Name = yukiMenu.Name
    
End

Public Sub _new(argButton As MenuButton, argGrids As GridView)
    
    initializeStarMenu(argButton)
    $grids = New NagatoGridsPlaylists(argGrids) As "Grids"
    
End
