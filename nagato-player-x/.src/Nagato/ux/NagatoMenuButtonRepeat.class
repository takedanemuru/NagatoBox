Inherits NagatoMenuButton

Public Sub _OnInitializeMenuButton()
    
    With Me._$menuButton
        .Tooltip = ("Repeat")
        .Picture = MikuruIcon.Get("button-synchronize")
    End With
    
End

Public Sub _OnClickMenuButton()
    
    NagatoSettingsMusic.Repeat = Not NagatoSettingsMusic.Repeat
    
End
