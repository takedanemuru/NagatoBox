' Gambas class file

Create Static

Property Read Paths As String

Private $path As String
Private $paths As String[] = [""]

Public Sub Parsing()
    
    Args.Begin(("Usage"))
        $path = Args.Get("p", "play", "play music", "path to play")
    $paths = Args.End()
    
End

Private Function Paths_Read() As String

    Return MikuruPath.Sanitize($paths.Add($path, 0))

End
