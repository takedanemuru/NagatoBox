Public Sub Main()
    
    NagatoArguments.Parsing()
    
    If NagatoDBus.IsUnique() Then
        NagatoQueue.Activate()
        HaruhiPlayer.Show() 
        If NagatoArguments.Paths Then NagatoQueue(MikuruSignal.ListAddMusic, NagatoArguments.Paths)
    Else
        NagatoDBus.NotUniqueSequence()
    End If
    
End
