' Gambas module file

Private Sub testQueue()
    
    Dim yukiPath As String
    
    For Each yukiPath In NagatoInterface.Paths
        Debug NagatoInterface.Queue[yukiPath]
    Next
    
End

Public Sub Main()
    
    NagatoArguments.Parsing()
    
    If NagatoDBus.IsUnique() Then
        NagatoQueue.Activate()
        HaruhiPlayer.Show() 
        If NagatoArguments.Paths <> Null Then NagatoQueue.AddMusics(NagatoArguments.Paths)
    Else
        NagatoDBus.Proxy.MoveToCurrentDesktop()
        If NagatoArguments.Paths <> Null Then NagatoDBus.Proxy.AddMusics(NagatoArguments.Paths)
    End If
    
    Debug NagatoQueue.Collection.Count
    
    testQueue()
    
End
