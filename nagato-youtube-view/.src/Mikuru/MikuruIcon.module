' Gambas module file

Public Sub _get(argIconName As String, Optional argSize As Integer = 16) As Picture
    
    Dim yukiPath As String = Subst("pictures/icons/&1.png", argIconName)
    Dim yukiImage As Image = Image.Load(yukiPath)
    
    Return yukiImage.Stretch(argSize, argSize).Picture
    
Catch
    Return Null
    
End
