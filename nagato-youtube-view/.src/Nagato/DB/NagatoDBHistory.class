' Gambas class file

Inherits NagatoDB

Public _$uniqueName As String = "YoutubeHistory"

Private $tableHistory As NagatoTableHistory

Public Sub Delete(argUrl As String)
    
    $tableHistory.DeleteHistory(argUrl)
    
    Raise Update
    
End

Public Sub DeleteRelated(argUrl As String)
    
    $tableHistory.DeleteRelatedHistories(argUrl)
    
    Raise Update
    
End

Public Sub ClearAll()
    
    $tableHistory.ClearAll()
    
    Raise Update
    
End

Public Sub Push(argUrl As String, argTitle As String)
    
    $tableHistory.DeleteHistory(argUrl)
    $tableHistory.PushHistory(argUrl, argTitle)
    
    Raise Update
    
End

Public Function GetAll() As Result
    
    Return $tableHistory.GetAll()
    
End

Public Sub _OnSetTable()
    
    $tableHistory = New NagatoTableHistory(Me._$connection)
    
End
