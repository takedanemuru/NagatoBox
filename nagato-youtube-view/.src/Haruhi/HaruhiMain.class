' Gambas class file

Private $mainContainer As NagatoMainContainer

Public Sub _new()
    
    $mainContainer = New NagatoMainContainer(VBox1)
    
End

Public Sub Form_Open()
    
    NagatoSettingsForm.Load(Me)
    
End

Public Sub Form_Close()
    
    Dim yukiMessage As String = ("YUKI.N > Are you sure to close application ?")
    
    If Message.Question(yukiMessage, ("Cancel"), ("Close")) <> 2 Then Stop Event
    NagatoSettingsForm.Save(Me)
    
End
