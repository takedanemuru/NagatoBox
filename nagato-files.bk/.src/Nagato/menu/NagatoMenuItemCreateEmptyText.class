' Gambas class file

Inherits NagatoMenuItem

Public Sub _InitializeItem()
    
    Dim yukiMenu As New Menu(Me._$parentMenu) As "MenuItem"
    
    With yukiMenu
        .Text = ("Create Empty Text")
        .Picture = MikuruIcon.Get("document")
    End With
    
End

Public Sub MenuItem_Click()
    
    File.Save(MikuruSafePath(Me._$path &/ "new_text.txt"), "")
    
End
