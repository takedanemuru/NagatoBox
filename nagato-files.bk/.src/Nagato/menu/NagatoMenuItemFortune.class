' Gambas class file

Inherits NagatoMenuItem

Public Sub _InitializeItem() 'override
    
    Dim yukiMenu As New Menu(Me._$parentMenu) As "MenuItem"
    
    With yukiMenu
        .Text = ("YUKI.N > ...")
        .Picture = MikuruIcon.Get("balloon")
        .Tag = Me._$path
    End With
    
End

Public Sub MenuItem_Click()
    
    If HaruhiPersona.Closed Then HaruhiPersona.Fortune()
    
End
