' Gambas class file

Inherits NagatoMenuItem


Public Sub _InitializeItem() 'override
    
    Dim yukiMenu As New Menu(Me._$parentMenu) As "MenuItem"
    
    With yukiMenu
        .Text = ("Image")
        .Picture = MikuruColorChip.Get(NagatoSettingsTileColor.Image)
    End With
    
End

Public Sub MenuItem_Click()
    
    If Not Dialog.SelectColor() Then NagatoSettingsTileColor.Image = Dialog.Color
    
End
