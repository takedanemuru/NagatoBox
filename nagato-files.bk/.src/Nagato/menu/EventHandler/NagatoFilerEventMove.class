' Gambas class file

Inherits NagatoFilerEvent

Public Sub _call(argSignal As Integer, argValues As Variant[])
    
    Select Case argSignal
        Case NagatoObject.MoveOpenDirectory
            Raise Request(NagatoFilerEvent.RequestMove, [argValues[0]])
        Case NagatoObject.MoveOpenDirectoryInNewTab
            TFEI.AddNewDirectory(argValues[0])
        Default
            Raise Request(NagatoFilerEvent.RequestMove, [Me._$log.GetMoveTo(argSignal)])
    End Select
    
End