' Gambas class file

Inherits NagatoMenuItem

Public Sub _InitializeItem() 'override
    
    Dim yukiMenu As New Menu(Me._$parentMenu) As "MenuItem"
    
    With yukiMenu
        .Text = ("DesktopEntry")
        .Picture = MikuruColorChip.Get(NagatoSettingsTileColor.DesktopEntry)
    End With
    
End

Public Sub MenuItem_Click()
    
    If Not Dialog.SelectColor() Then NagatoSettingsTileColor.DesktopEntry = Dialog.Color
    
End
