' Gambas class file

Inherits NagatoMenuItem

Public Sub _InitializeItem() 'override
    
    Dim yukiMenu As New Menu(Me._$parentMenu) As "MenuItem"
    
    With yukiMenu
        .Text = ("Unarchive")
        .Picture = MikuruIcon.Get("hard-drive-upload")
        .Tag = Me._$path
    End With
    
End

Public Sub MenuItem_Click()
    
    Shell Subst$("nagato-extractor &1", Me._$path)
    
End
