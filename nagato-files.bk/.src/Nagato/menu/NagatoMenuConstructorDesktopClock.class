' Gambas class file

Inherits NagatoMenuConstructor

Public Sub _call()
    
    Me._$rootMenu.Children.Clear()
    Me._AddContextMenu(("Clock Format"), 0, MikuruIcon.Get("alarm-clock"))
    
End

Public Sub ContextMenu_Click()
    
    HaruhiClockFormat.SetTarget(HaruhiClockFormat.TargetDesktop)
    HaruhiClockFormat.ShowDialog() 
    
End
