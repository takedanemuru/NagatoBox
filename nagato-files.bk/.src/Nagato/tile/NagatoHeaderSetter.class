Public Sub _call(argTile As Picture, argMode As Integer) As Picture
    
    Dim yukiImage As Image
    
    If argMode = MikuruTileMode.ModeNull Then Return argTile
    
    yukiImage = argTile.Image.Copy()
    
    Paint.Begin(yukiImage)
        NagatoLabelPainterFacade.Paint(argMode, "")
    Paint.End()
    
    Return yukiImage.Picture
    
End
