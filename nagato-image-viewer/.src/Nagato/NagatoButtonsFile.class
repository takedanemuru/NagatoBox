' Gambas class file

Inherits NagatoButtons

Public Sub _OnInitializeButtons()
    
    Me._AddButton("Open File", MikuruSignal.FileOpen, Stock["small/open"])
    Me._AddButton("Back", MikuruSignal.FileBack, Stock["small/previous"])
    Me._AddButton("Next", MikuruSignal.FileNext, Stock["small/next"])
    Me._AddButton("Rotate Left", MikuruSignal.ViewRotateLeft, Stock["small/rotate-left"])
    Me._AddButton("Rotate Right", MikuruSignal.ViewRotateRight, Stock["small/rotate-right"])
    Me._AddButton("Zoom : Normal", MikuruSignal.ZoomNormal, Stock["small/zoom-normal"])
    Me._AddButton("Zoom : Fit", MikuruSignal.ZoomFit, Stock["small/zoom-fit"])
    Me._AddButton("Zoom-In", MikuruSignal.ZoomIn, Stock["small/zoom-in"])
    Me._AddButton("Zoom-Out", MikuruSignal.ZoomOut, Stock["small/zoom-out"])

End
