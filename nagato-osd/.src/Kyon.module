' Gambas module file

Public Sub Main()
    
    Application.Font.Size = NagatoSettingsFonts.SizeContent
    
    If DBus.Session.Applications.Exist("org.gambas.nagato-osd") Then
        Message.Error(("YUKI.N > nagato-osd has been executed."), ("OK"))
    Else
        NagatoDBus.Register()
        MikuruHidden.Ensure()
        HaruhiMain.Show()
    End If
    
End
