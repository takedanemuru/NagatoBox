Private $painterTextClock As New NagatoSectionPainterClock
Private $painterAnalogClock As New NagatoPainterAnalogClock

Public Sub _call(argY As Integer) As Integer
    
    If NagatoSettingsClock.Type = NagatoSettingsClock.TypeAnalog Then
        Return $painterAnalogClock(argY)
    Else
        Return $painterTextClock(argY)
    Endif
    
End
