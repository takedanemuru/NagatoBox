' Gambas class file

Inherits NagatoMenuItem

Public Sub _OnMenuItemClick()
    
    NagatoSettingsRss.ShowThumbnail = Not NagatoSettingsRss.ShowThumbnail
    
End

Public Sub _OnInitializeMenuItem()
    
    With Me._$menuItem
        .Text = ("Show Thumbnail")
        .Picture = MikuruCheckButton(NagatoSettingsRss.ShowThumbnail)
    End With
    
End
