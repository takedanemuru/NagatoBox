Inherits NagatoMenuItem

Public Sub _OnMenuItemClick()
    
    NagatoSettingsClock.Type = NagatoSettingsClock.TypeAnalog
    
End

Public Sub _OnInitializeMenuItem()
    
    With Me._$menuItem
        .Text = ("Type : Analog")
        .Picture = MikuruCheckButton(NagatoSettingsClock.Type = NagatoSettingsClock.TypeAnalog)
    End With
    
End
