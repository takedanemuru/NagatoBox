' Gambas class file

Create Static

Private $tabStrip As TabStrip
Private $bookmark As NagatoInfoBookmarks
Private $search As NagatoInfoSearch

Public Sub Activate(argTabstrip As TabStrip)
    
    $tabStrip = argTabstrip
    $bookmark = New NagatoInfoBookmarks($tabStrip)
    $search = New NagatoInfoSearch($tabStrip)
    
    $tabStrip.Index = 0
    
End

Public Sub RefreshBookmark()
    
    $tabStrip.Refresh()
    
End
