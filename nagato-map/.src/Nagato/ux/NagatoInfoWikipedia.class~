' Gambas class file

Private $tabstrip As TabStrip

Private Sub initializeTabStrip(argContainer As Container)
    
    $tabstrip = New TabStrip(argContainer)
    
    With $tabstrip
        .Expand = True
        .Count = 2
        .Arrangement = Arrange.Vertical
    End With
    
End

Public Sub _new(argContainer As Container)
    
    initializeTabStrip(argContainer)
    
End
