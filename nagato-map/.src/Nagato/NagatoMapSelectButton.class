Inherits NagatoMenuPipe

Public Sub _new(argContainer As Container)
    
    Dim yukiComboBox As New ComboBox(argContainer) As "ComboBox"
    
    With yukiComboBox
        .W = 240
        .ReadOnly = True
        .List = ["OpenStreetMap", "Water Color", "Spinal"]
    End With
    
End

Public Sub ComboBox_Click()
    
    MikuruOpenStreetMap.SetIndex(Last.Index)
    
    Raise Signal(NagatoMenuPipe.MapSelect, [Last.Index])
    
End
