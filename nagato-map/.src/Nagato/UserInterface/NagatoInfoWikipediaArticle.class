Inherits NagatoInfo

Private $wikipediaWebView As WebView

Public Sub _InitializeWidgets()
    
    Me._$tabStrip.Index = 2
    
    $wikipediaWebView = New WebView(Me._$tabStrip)
    
    With $wikipediaWebView
        .Expand = True
        .Background = Color.Pink
    End With
    
End

Public Sub SetUrl(argUrl As String)
    
    $wikipediaWebView.Url = argUrl
    
End
