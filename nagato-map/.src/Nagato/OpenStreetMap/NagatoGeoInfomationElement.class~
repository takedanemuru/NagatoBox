' Gambas class file

Property Latitude As Float
Property Longitude As Float
Property Type As String
Property Title As String
Property MobileUrl As String

Private $latitude As Float
Private $longitude As Float
Private $type As String
Private $title As String
Private $mobileUrl As String

Private Function Title_Read() As String

    Return $title

End

Private Sub Title_Write(Value As String)

    $title = Value

End

Private Function Type_Read() As String

    Return $type

End

Private Sub Type_Write(Value As String)

    $type = Value

End

Private Function MobileUrl_Read() As String

    Return $mobileUrl

End

Private Sub MobileUrl_Write(Value As String)

    $mobileUrl = Value

End


Private Function Latitude_Read() As Float

    Return $latitude

End

Private Sub Latitude_Write(Value As Float)

    $latitude = Value

End

Private Function Longitude_Read() As Float

    Return $longitude

End

Private Sub Longitude_Write(Value As Float)

    $longitude = Value

End
