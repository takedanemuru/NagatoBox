' Gambas module file

Property Read Grid As Integer
Property Read GridBackgroundStripe As Integer
Property Read GridBackgroundPink As Integer

Private Function Grid_Read() As Integer

    Return IIf(desktop.Resolution > 100, 16, 8)

End

Private Function GridBackgroundStripe_Read() As Integer

    Return Color.Merge(Color.White, Color.Blue, 0.05)

End

Private Function GridBackgroundPink_Read() As Integer

    Return Color.Merge(Color.White, Color.Pink, 0.2)

End
