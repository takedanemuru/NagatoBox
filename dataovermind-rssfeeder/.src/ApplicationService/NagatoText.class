Public Function Sanitize(argSource As String, ...) As String
    
    Dim yukiResult As String = argSource
    Dim yukiStringToRemove As String
    
    For Each yukiStringToRemove In Param
        yukiResult = Replace$(yukiResult, yukiStringToRemove, "")
    Next
    
    Return yukiResult
    
End
