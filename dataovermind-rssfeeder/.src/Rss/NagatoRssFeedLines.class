' Gambas class file

Create Static

Private Const BBCTop As String = "http://feeds.bbci.co.uk/news/rss.xml"
Private Const BBCWorld As String = "http://feeds.bbci.co.uk/news/world/rss.xml"
Private Const BBCTechnology As String = "http://feeds.bbci.co.uk/news/technology/rss.xml"
Private Const TASS As String = "http://tass.ru/en/rss/v2.xml"

Public Function GetDefault() As Collection
    
    Dim yukiLines As New Collection
    
    With yukiLines
        .Add(TASS, "TASS")
        .Add(BBCTop, "BBC Top")
        .Add(BBCWorld, "BBC World")
        .Add(BBCTechnology, "BBC Technology")
    End With
    
    Return yukiLines
    
End
