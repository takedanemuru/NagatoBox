' Gambas class file

Create Static

Public Sub Create(argPathsLine As String, argDestinationPath As String)
    
    Shell Subst$("patool --verbose create &1 &2", argDestinationPath, argPathsLine) Wait
    
    If Exist(argDestinationPath) Then
        Message.Info(Subst$("YUKI.N > &1 has been created.", File.Name(argDestinationPath)))
    Else
        Message.Error("YUKI.N > Some error occurred. Failed to create archive file.")
    End If
    
End
