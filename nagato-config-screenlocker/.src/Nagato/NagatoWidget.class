Private Const Height As Integer = 36
Private Const Padding As Integer = 2

Public _$hBox As HBox

Public Sub _InitializeHBox(argContainer As Container)
    
    Me._$hBox = New HBox(argContainer)
    
    With Me._$hBox
        .Height = Height
        .Padding = Padding
    End With
    
End
