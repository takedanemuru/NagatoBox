' Gambas class file

Inherits NagatoMenuButton

Private Const MenuName As String = "MenuNameDownload"

Public Function _OptionalOnGetMenuName() As String
    
    Return MenuName
    
End

Public Sub _OptionalOnInitializeMenuButton()
    
    With Me._$menuButton
        .Picture = MikuruIcon["hard-drive-download"]
        .Tooltip = ("Download")
        .Visible = False
    End With
    
End

Public Sub _OptionalOnInitializePulldownMenu()
    
    Dim yukiMenu As NagatoPullDownMenuDownload
    
    yukiMenu = New NagatoPullDownMenuDownload(MenuName) As "TFEI"
    
End

Public Sub _OnCall(argSignal As Integer, argValues As Variant[])
    
    Select Case argSignal
        Case MikuruSignal.WebViewLoaded
            Me._$menuButton.Visible = argValues[0]
    End Select
    
End
