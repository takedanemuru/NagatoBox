' Gambas class file

Create Static

Public Const ObjectPath As String = "/org/nagato/text"

Public Function IsUnique() As Boolean
    
    Return Not DBus.Session.Applications.Exist("org.gambas.nagato.text")
    
End

Public Sub Register()
    
    DBus.Session.Register(NagatoInterface, Me.ObjectPath)
    
End

Public Sub Unregister()
    
    DBus.Session.Unregister(NagatoInterface)
    
End
