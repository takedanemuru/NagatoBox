Private $panel As NagatoContainerMenuPanelNetwork
Private $graph As NagatoGraphNetwork
Private $gridView As NagatoGridsWifiAccessPoints

Public Sub Tick()
    
    $graph.Refresh()
    $panel.Refresh()
    
End

Public Sub _new(argContainer As Container)
    
    $graph = New NagatoGraphNetwork(argContainer)
    $panel = New NagatoContainerMenuPanelNetwork(argContainer)
    $gridView = New NagatoGridsWifiAccessPoints(argContainer)
    
End
