' Gambas class file

Property Read Current As String
Property Index As Integer

Private $targetIndex As Integer = 0
Private $headers As String[] = ["id", "status", "name", "cpu", "rss", "virtual"]

Private Function Current_Read() As String

    Return $headers[$targetIndex]

End

Private Function Index_Read() As Integer

    Return $targetIndex

End

Private Sub Index_Write(Value As Integer)

    If Value > $headers.Max Then Return
    
    $targetIndex = Value

End
