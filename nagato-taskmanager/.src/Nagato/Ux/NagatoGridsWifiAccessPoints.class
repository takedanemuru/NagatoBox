Private $gridView As GridView

Private Sub setData()
    
    Dim yukiIndex As Integer
    Dim yukiAccessPoint As NagatoAccessPoint
    
    For yukiIndex = 0 To NagatoAccessPoints.Max
        Inc $gridView.Rows.Count
        yukiAccessPoint = NagatoAccessPoints[yukiIndex]
        $gridView[$gridView.Rows.Max, 0].Text = yukiAccessPoint.HwAddress
        $gridView[$gridView.Rows.Max, 1].Text = yukiAccessPoint.WpaFlags
        $gridView[$gridView.Rows.Max, 2].Text = yukiAccessPoint.MaxBitrate
        $gridView[$gridView.Rows.Max, 3].Text = yukiAccessPoint.Strength
        $gridView[$gridView.Rows.Max, 4].Text = yukiAccessPoint.Mode
    Next
    
End

Public Sub _new(argContainer As Container)
    
    $gridView = New GridView(argContainer)
    
    $gridView.Columns.Count = 5
    $gridView.Header = GridView.Horizontal
    $gridView.Columns[0].Text = ("HwAddress")
    $gridView.Expand = True
    
    If NagatoAccessPoints.Max >= 0 Then setData
    
End
