Private $serchBox As ButtonBox

Public Sub _new(argContainer As Container)
    
    $serchBox = New ButtonBox(argContainer) As "SearchBox"
    
    With $serchBox
        .Border = False
        .W = 200
       .Picture = MikuruIcon.Get("search")
       .Visible = True
       .Text = ""
    End With
    
End

Public Sub SearchBox_Change()
    
    MikuruProcessTemporaryVariables.ChangeFilter($serchBox.Text)
    
End
