Inherits NagatoToolbar

Private $spacer As NagatoWidgetSpacer
Private $cancelButton As NagatoWidgetButtonCancel
Private $applyButton As NagatoWidgetButtonApply

Public Sub _InitializeWidgets()
    
    $spacer = New NagatoWidgetSpacer(Me._$HBox)
    $cancelButton = New NagatoWidgetButtonCancel(Me._$HBox)
    $applyButton = New NagatoWidgetButtonApply(Me._$HBox) 
    
End

Public Sub Activate()
    
    $applyButton.Activate()
    
End
