' Gambas class file

Inherits NagatoSignal

Private $tabStrip As TabStrip
Private $iconView As IconView
Private $grids As NagatoCursorThemeGrids

Private Sub setIcons()
    
    Dim yukiItemName As String
    Dim yukiIcon As Picture
    
    $iconView.Clear()
    
    For Each yukiItemName In Stock.Icons
        yukiIcon = Stock[Subst$("&1/&2", "huge", yukiItemName)]
        $iconView.Add(yukiItemName, "", yukiIcon)
    Next

    $iconView.Refresh()

End

Public Sub _new(argTabStrip As TabStrip)
    
    $tabStrip = argTabStrip
    $tabStrip.Index = 3
    
    $grids = New NagatoCursorThemeGrids($tabStrip) As "TFEI"
    
    $tabStrip.Index = 0
    
End
